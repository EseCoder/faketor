{
  "compilerOptions": {
    "module": "NodeNext",
    "moduleResolution": "Node",
    "target": "es5",
    "baseUrl": "src/bin",
    "outDir": "",
    "sourceMap": true,
    "noImplicitAny": true,
    "allowSyntheticDefaultImports": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false
  },
  "exclude": [
    "node_modules"
  ]
}